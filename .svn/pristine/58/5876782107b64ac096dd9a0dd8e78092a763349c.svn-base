<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Quiz Saber</title>
    @Styles.Render("~/Content/css")

    @RenderSection("CssHeader", required: false)

    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/Quiz.css" rel="stylesheet" />


</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">


        <div class="container">

            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <div>
                    <img src="~/Images/can-stock-photo_fundo_Completo.jpg" width="40" style="display:inline; float:left; margin-right:15px; margin-top:5px" />
                    @*<img src="~/Images/can-stock-photo_csp9965349.jpg" width="30" style="display:inline; float:left; margin-right:15px; margin-top:10px" />*@
                    @*<img src="~/Images/science-103112-003-617x416.jpg" width="30" style="display:inline; float:left; margin-right:15px; margin-top:10px" />*@
                    @*<img src="~/Images/o-cérebro-está-pensando-as-engrenagens-2892677.jpg" width="30" style="display:inline; float:left; margin-right:15px; margin-top:10px" />*@
                    @*<img src="~/Images/geografia_mapa.jpg" width="30" style="display:inline; float:left; margin-right:15px; margin-top:10px" />*@

                    @*<img src="~/Images/língua.materia.jpg" width="40" style="display:inline; float:left; margin-right:15px; margin-top:10px" />*@
                    @Html.ActionLink("Quiz Saber", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                </div>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    @*<li>@Html.ActionLink("Início", "Index", "Home")</li>*@
                    @*<li>@Html.ActionLink("Sobre", "About", "Home")</li>
                        <li>@Html.ActionLink("Contato", "Contact", "Home")</li>*@
                    @*<li>@Html.ActionLink("Conceito", "Index", "Conceito")</li>*@
                </ul>

                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>

    <div class="container body-content">
        @RenderBody()

        <hr />
        <footer>
            <p style="text-align:center">&copy; @DateTime.Now.Year - II CODE RACE - Antônio Meneghetti Faculdade</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    <script>
        $(document).ready(function () {
            $.ajax({
                url: '/Quiz/ConsultaProfile',
                type: "POST",
                contentType: "application/json; charset=utf-8",
                dataType: 'json',
                success: function (data) {
                    $('#pontosUser').html(data.Pontuacao);
                    $('#nivelUser').html(data.Nivel);

                },
                error: function () {

                }
            });
        });
    </script>
    @RenderSection("scripts", required: false)
</body>
</html>
